  <body>
    <div class="titlebar">
      <div class="chef">
        <img src="../chef.png" height="100%" width="100%" alt="" />
      </div>
      <div class="title">Tasty Tender</div>
      
    </div>
    <div class="searchcontainer">
      <input
        type="search"
        placeholder="search the ingredients"
        class="search"
      />
      <div class="search-results"></div>
    </div>

    <div id="mySidenav" class="sidenav">
      <a href="javascript:void(0)" class="closebtn">&times;</a>
      <div>
        <img class="sidebarprofile" src="../user_6994618.png" alt="profile" />
      </div>
      <div class="username" ></div>
      <a href="#">Home</a>
      <a href="/logout">Logout</a>
      <a href="" id="postlink">Upload Recipes</a>
      <a href="#" class="savedrecipe">Saved Recipes</a>
      <a href="#" id="usermanual">User Manual</a>
    </div>
    <top class="top"><svg xmlns="http://www.w3.org/2000/svg" height="24px" viewBox="0 -960 960 960" width="24px" fill="#e8eaed"><path d="m296-224-56-56 240-240 240 240-56 56-184-183-184 183Zm0-240-56-56 240-240 240 240-56 56-184-183-184 183Z"/></svg>TOP</top>

    <span class="openbtn">&#9776; ALL</span>
    <div class="styling">
    <div class="ingredientscontainer">
      <div class="ingredienttype">
        <button class="ingtype">protein</button>
        <button class="ingtype">Meat and Poultry</button>
        <button class="ingtype">Seafood</button>
        <button class="ingtype">Eggs</button>
        <button class="ingtype">Dairy</button>
        <button class="ingtype">Plant-Based Proteins</button>
        <button class="ingtype">Fruits and Vegetables</button>
        <button class="ingtype">Herbs and Spices</button>
        <button class="ingtype">Oils and Fats</button>
        <button class="ingtype">Flour and Baking Ingredients</button>
        <button class="ingtype">Liquid Ingredients</button>
        <button class="ingtype">Condiments and Sauces</button>
        <button class="ingtype">Dairy Alternatives</button>
        <button class="ingtype">Nuts and Seeds</button>
        <button class="ingtype">Sweets and Snacks</button>
        <button class="ingtype">Miscellaneous Ingredients</button>
      </div>
      <div class="ingredientslist"></div>
      <div class="addstyle">
      <button class="addtocart">add to cart</button>
    </div>
    </div>
    <div class="cart">
    <div  class="cartlogo"><img class="cartimage" src="../cart.png" alt="cart" /> 
  </div>
      <div class="ingredientstitle">Ingredients List</div>
      <div class="choseingredients"></div>
      <div class="constriction">
        <div class="cuisine">CusineSelection</div>
        <div class="options-container hidden">
          <div class="option">
            <input
              type="checkbox"
              id="italian"
              name="cuisine"
              value="Italian"
            />
            <label for="italian">Italian</label>
          </div>
          <div class="option">
            <input
              type="checkbox"
              id="chinese"
              name="cuisine"
              value="Chinese"
            />
            <label for="chinese">Chinese</label>
          </div>
          <div class="option">
            <input type="checkbox" id="indian" name="cuisine" value="Indian" />
            <label for="indian">Indian</label>
          </div>
          <div class="option">
            <input
              type="checkbox"
              id="mexican"
              name="cuisine"
              value="Mexican"
            />
            <label for="mexican">Mexican</label>
          </div>
          <div class="option">
            <input
              type="checkbox"
              id="japanese"
              name="cuisine"
              value="Japanese"
            />
            <label for="japanese">Japanese</label>
          </div>
          <div class="option">
            <input type="checkbox" id="thai" name="cuisine" value="Thai" />
            <label for="thai">Thai</label>
          </div>
          <div class="option">
            <input type="checkbox" id="french" name="cuisine" value="French" />
            <label for="french">French</label>
          </div>
          <div class="option">
            <input type="checkbox" id="greek" name="cuisine" value="Greek" />
            <label for="greek">Greek</label>
          </div>
          <div class="option">
            <input
              type="checkbox"
              id="spanish"
              name="cuisine"
              value="Spanish"
            />
            <label for="spanish">Spanish</label>
          </div>
          <div class="option">
            <input
              type="checkbox"
              id="mediterranean"
              name="cuisine"
              value="Mediterranean"
            />
            <label for="mediterranean">Mediterranean</label>
          </div>
        </div>
        <div class="preparationtime">Maximum Preparation Time (minutes)</div>
        <div class="preparationtimecontainer hidden">
          <div class="time">10</div>
          <div class="time">20</div>
          <div class="time">30</div>
          <div class="time">40</div>
          <div class="time">50</div>
          <div class="time">60</div>
          <div class="time">60+</div>
        </div>
        <div class="dietaryrestriction">Dietary Restriction</div>
        <div class="dietcontainer hidden">
          <div class="diet">
            <input
              type="checkbox"
              id="vegetarian"
              name="diet"
              value="Vegetarian"
            />
            <label for="vegetarian">Vegetarian</label>
          </div>
          <div class="diet">
            <input type="checkbox" id="vegan" name="diet" value="Vegan" />
            <label for="vegan">Vegan</label>
          </div>
          <div class="diet">
            <input
              type="checkbox"
              id="glutenFree"
              name="diet"
              value="Gluten-Free"
            />
            <label for="glutenFree">Gluten-Free</label>
          </div>
          <div class="diet">
            <input
              type="checkbox"
              id="dairyFree"
              name="diet"
              value="Dairy-Free"
            />
            <label for="dairyFree">Dairy-Free</label>
          </div>
          <div class="diet">
            <input type="checkbox" id="nutFree" name="diet" value="Nut-Free" />
            <label for="nutFree">Nut-Free</label>
          </div>
          <div class="diet">
            <input type="checkbox" id="soyFree" name="diet" value="Soy-Free" />
            <label for="soyFree">Soy-Free</label>
          </div>
          <div class="diet">
            <input type="checkbox" id="keto" name="diet" value="Keto" />
            <label for="keto">Keto</label>
          </div>
          <div class="diet">
            <input type="checkbox" id="paleo" name="diet" value="Paleo" />
            <label for="paleo">Paleo</label>
          </div>
          <div class="diet">
            <input
              type="checkbox"
              id="lowFODMAP"
              name="diet"
              value="Low FODMAP"
            />
            <label for="lowFODMAP">Low FODMAP</label>
          </div>
          <div class="diet">
            <input type="checkbox" id="kosher" name="diet" value="Kosher" />
            <label for="kosher">Kosher</label>
          </div>
          <div class="diet">
            <input type="checkbox" id="halal" name="diet" value="Halal" />
            <label for="halal">Halal</label>
          </div>
          <div class="diet">
            <input
              type="checkbox"
              id="pescatarian"
              name="diet"
              value="Pescatarian"
            />
            <label for="pescatarian">Pescatarian</label>
          </div>
          <div class="diet">
            <input type="checkbox" id="lowCarb" name="diet" value="Low-Carb" />
            <label for="lowCarb">Low-Carb</label>
          </div>
          <div class="diet">
            <input
              type="checkbox"
              id="lowSodium"
              name="diet"
              value="Low-Sodium"
            />
            <label for="lowSodium">Low-Sodium</label>
          </div>
          <div class="diet">
            <input type="checkbox" id="diabetic" name="diet" value="Diabetic" />
            <label for="diabetic">Diabetic</label>
          </div>
          <div class="diet">
            <input type="checkbox" id="none" name="diet" value="None" />
            <label for="diabetic">None</label>
          </div>
        </div>
        <div class="buttoncontainer">
          <button class="submitfinal">submit</button>
        </div>
      </div>
    </div>
  </div>
       
    <div class="showrecipe">
      <div class="close"><?xml version="1.0" ?><!DOCTYPE svg  PUBLIC '-//W3C//DTD SVG 1.1//EN'  'http://www.w3.org/Graphics/SVG/1.1/DTD/svg11.dtd'><svg height="30" id="Layer_1" style="enable-background:new 0 0 512 512;" fill="white" version="1.1" viewBox="0 0 512 512" width="30" xml:space="preserve" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"><path d="M443.6,387.1L312.4,255.4l131.5-130c5.4-5.4,5.4-14.2,0-19.6l-37.4-37.6c-2.6-2.6-6.1-4-9.8-4c-3.7,0-7.2,1.5-9.8,4  L256,197.8L124.9,68.3c-2.6-2.6-6.1-4-9.8-4c-3.7,0-7.2,1.5-9.8,4L68,105.9c-5.4,5.4-5.4,14.2,0,19.6l131.5,130L68.4,387.1  c-2.6,2.6-4.1,6.1-4.1,9.8c0,3.7,1.4,7.2,4.1,9.8l37.4,37.6c2.7,2.7,6.2,4.1,9.8,4.1c3.5,0,7.1-1.3,9.8-4.1L256,313.1l130.7,131.1  c2.7,2.7,6.2,4.1,9.8,4.1c3.5,0,7.1-1.3,9.8-4.1l37.4-37.6c2.6-2.6,4.1-6.1,4.1-9.8C447.7,393.2,446.2,389.7,443.6,387.1z"/></svg></div>
      <div class="recipeblock">
      <div class="recipetitle">title</div>
      <div class="backgroundforrecipeimage">
        <div class="recipeimage"></div>
      </div>

      <div class="recipeclicked">
        <div class="ingredientstitleselected">Ingredients List:</div>
        <div class="ingredientsforselected"></div>
        <div class="proceduretitle">Procedure:</div>
        <div class="procedure"></div>
        
        <div class="readcontainer">
          <div class="control">
          <button id="pauseButton">&#x23F8</button>
          <button id="resumeButton">&#9658</button>
         </div>
          <img
          class="read"
          src="../speech.png"
          width="75px"
          height="75px"
          alt="Read Aloud"
        />
        </div>
        <div class="ratingtitle">Rating:</div>
        <div class="rating">
          <span class="star" data-value="1">&#9733;</span>
          <span class="star" data-value="2">&#9733;</span>
          <span class="star" data-value="3">&#9733;</span>
          <span class="star" data-value="4">&#9733;</span>
          <span class="star" data-value="5">&#9733;</span>
        </div>
        <div class="likes"></div>
        <div class="heart-button">
          <img
            src="../heart.png"
            alt="doubleclickforsaving"
            class="nonelike"
          />
        </div>
        <div class="review">Write your comments:</div>
        <input type="text" id="userreview" class="reviewfordish" />
        <button id="userreviewbutton" type="submit">
          submit your rating and comment
        </button>
        <div class="comments">Comments</div>
        <div class="commentsfordish"></div>
        <div class="ratingbyothers">Ratings</div>
        <div class="ratingfordish"></div>
      </div>
    </div>
    </div>
